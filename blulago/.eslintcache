ainer>\r\n    )\r\n}\r\n\r\nconst StyledAboutTitle = styled.h2`\r\ncolor: #595959;\r\nfont-family: 'Lobster Two', cursive;\r\n`\r\n\r\nconst AboutUsDiv = styled.div`\r\nborder: none;\r\nbackground-color: #e3dfdb;\r\nwidth: 70vw;\r\nheight: 60vh;\r\nmargin: 1rem 1rem;\r\ndisplay: flex;\r\nflex-direction: column;\r\njustify-content: space-around;\r\nalign-items: center;\r\ntext-align: center;\r\npadding: 0rem 4rem;\r\n@media (max-width: 900px){\r\n    min-width: 93vw;\r\n    height: fit-content;\r\n    text-align: normal;\r\n    padding: 0rem;\r\n    margin: 1rem 1rem;\r\n}\r\n@media (max-width: 600px){\r\n    min-width: 89vw;\r\n    min-height: 650px;\r\n    margin-right: 3rem;\r\n    padding: 0rem 1rem;\r\n}\r\n\r\n`\r\n\r\nconst PictureContainer = styled.div`\r\nmin-height: 60vh;\r\n\r\n`\r\n\r\nconst AboutUsPic = styled.div`\r\nborder: none;\r\nwidth: 35vw;\r\nheight: 29vh;\r\nmargin-top: 1rem;\r\nmargin-right: 2rem;\r\nimg{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n    @media (max-width: 768px){\r\n        \r\n    }\r\n}\r\n@media (max-width: 900px){\r\n    \r\n    width: 93vw;\r\n    margin: 0rem 1rem;\r\n    &:first-child {\r\n        margin-bottom: 1rem;\r\n    }\r\n}\r\n\r\n@media (max-width: 600px){\r\n    width: 89vw;\r\n}\r\n`\r\n\r\nconst FlexContainer = styled.div`\r\ndisplay: flex;\r\nz-index: 0;\r\n@media (max-width: 900px){\r\n    flex-direction: column;\r\n}\r\n`\r\n\r\nexport default AboutUs\r\n","C:\\Users\\User\\Desktop\\Blulago-react\\blulago-react\\blulago\\src\\components\\Checkout\\FormInput.jsx",[],"C:\\Users\\User\\Desktop\\Blulago-react\\blulago-react\\blulago\\src\\components\\Checkout\\Review.jsx",[],"C:\\Users\\User\\Desktop\\Blulago-react\\blulago-react\\blulago\\src\\components\\Weather.jsx",["112"],"import React,{ useState, useEffect} from 'react'\r\nimport axios from 'axios'\r\nimport styled from 'styled-components'\r\n\r\nconst Weather = () => {\r\n    const [getWeather, setGetWeather] = useState(null)\r\n    const [isDay, setIsDay] = useState(1)\r\n    console.log(getWeather)\r\n    \r\n   \r\n    \r\n    useEffect(() => {\r\n        axios.get('https://api.weatherapi.com/v1/current.json?key=af7640e0a70d46869a5132026210201&q=Troyes')\r\n        .then(data => {\r\n            setGetWeather(data)\r\n            setIsDay(getWeather.data.current.is_day)\r\n        })\r\n        .catch(error => console.log(error))\r\n        \r\n\r\n    }, [])\r\n    return (\r\n        <>\r\n            {getWeather &&(\r\n                <WeatherContainer>\r\n                    <TextContainer>\r\n            <h3>Lake Current Weather</h3>\r\n            <p>Conditions: {getWeather.data.current.condition.text}</p>\r\n            <p>Tempreture: {getWeather.data.current.temp_c} Celsius</p>\r\n            <p>Pressure: {getWeather.data.current.pressure_mb} Millibar</p>\r\n            <p>Wind Direction: {getWeather.data.current.wind_dir}</p>\r\n            <p>Wind Speed: {getWeather.data.current.wind_mph} mph</p>\r\n            </TextContainer>\r\n            {isDay ? (<StyledImg src=\"https://www.metoffice.gov.uk/binaries/content/gallery/metofficegovuk/hero-images/weather/cloud/cumulus-cloud.jpg\" alt=\"day\"/>) : \r\n            <StyledImg src=\"https://cdn.lynda.com/course/163609/163609-637286158074364266-16x9.jpg\" alt=\"night\"/>}\r\n            <WeatherIcon src={getWeather.data.current.condition.icon} alt=\"weather icon\"/>\r\n            </WeatherContainer>\r\n            )}\r\n            \r\n            </>\r\n        \r\n    )\r\n}\r\n\r\nconst StyledImg = styled.img`\r\nwidth: 100%;\r\nheight: 100%;\r\nz-index: -1;\r\nposition: absolute;\r\ntop: 0%;\r\nfilter: brightness(55%);\r\n`\r\n\r\nconst WeatherContainer = styled.div`\r\nwidth: 100%;\r\nheight: 100%;\r\nposition: relative;\r\nh3{\r\n    color: #eeeeee;\r\n    padding-top: 0.5rem;\r\n}\r\np{\r\n    color: #eeeeee;\r\n    padding-top: 0.5rem;\r\n}\r\n\r\n`\r\nconst TextContainer = styled.div`\r\nwidth: 100%;\r\ntext-align: center;\r\n\r\n`\r\n\r\nconst WeatherIcon = styled.img`\r\nmax-width: 100px;\r\nmax-height: 100px;\r\nposition: absolute;\r\nright: 32%;\r\nbottom: -9%;\r\nleft: 3%;\r\ntop: 30%;\r\n@media (max-width: 600px){\r\n    max-width: 60px;\r\n    max-height: 60px;\r\n}\r\n`\r\n\r\nexport default Weather\r\n",{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":4,"column":8,"nodeType":"119","messageId":"120","endLine":4,"endColumn":15},{"ruleId":"121","severity":1,"message":"122","line":21,"column":8,"nodeType":"123","endLine":21,"endColumn":10,"suggestions":"124"},"no-native-reassign",["125"],"no-negated-in-lhs",["126"],"no-unused-vars","'LakeTwo' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getWeather.data'. Either include it or remove the dependency array. You can also replace multiple useState variables with useReducer if 'setIsDay' needs the current value of 'getWeather.data'.","ArrayExpression",["127"],"no-global-assign","no-unsafe-negation",{"desc":"128","fix":"129"},"Update the dependencies array to be: [getWeather.data]",{"range":"130","text":"131"},[610,612],"[getWeather.data]"]